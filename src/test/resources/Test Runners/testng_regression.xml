<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Opencart App Test Regression Suite" thread-count="4"
	parallel="tests" verbose="4">


	<listeners>
		<listener
			class-name="com.qa.opencart.listners.ExtentReportListners" />
			<listener
			class-name="com.qa.opencart.listners.TestAllureListener" />
	</listeners>

	
	<test name="Login Page Test">
		
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test>
	
	<test name="Account Page Test">
		
		<classes>
			<class name="com.qa.opencart.tests.AccountPageTest" />
		</classes>
	</test>
	<test name="product Info Page Test">
		
		<classes>
			<class name="com.qa.opencart.tests.ProductInfoTest" />
		</classes>
	</test>

	

</suite>	